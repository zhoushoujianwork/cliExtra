{
  "version": "2.0",
  "metadata": {
    "name": "全栈Web应用开发流程",
    "description": "前后端分离的Web应用开发协作工作流",
    "namespace": "webapp_dev",
    "created_at": "2025-01-20T10:00:00Z",
    "updated_at": "2025-01-20T10:00:00Z"
  },
  "roles": {
    "backend_engineer": {
      "name": "后端工程师",
      "description": "负责API开发和后端服务",
      "tools": ["git", "docker", "database"],
      "responsibilities": ["API设计", "数据库设计", "业务逻辑"]
    },
    "frontend_engineer": {
      "name": "前端工程师", 
      "description": "负责前端开发和用户界面",
      "tools": ["git", "npm", "webpack"],
      "responsibilities": ["UI实现", "前端逻辑", "用户交互"]
    }
  },
  "nodes": {
    "start": {
      "id": "start",
      "type": "start",
      "title": "项目启动"
    },
    "api_design": {
      "id": "api_design",
      "type": "task",
      "title": "API设计",
      "owner": "backend_engineer",
      "estimated_time": "8h",
      "deliverables": ["API文档", "接口规范"]
    },
    "ui_design": {
      "id": "ui_design",
      "type": "task",
      "title": "UI设计",
      "owner": "frontend_engineer", 
      "estimated_time": "6h",
      "deliverables": ["UI设计稿", "交互原型"]
    },
    "backend_dev": {
      "id": "backend_dev",
      "type": "task",
      "title": "后端开发",
      "owner": "backend_engineer",
      "estimated_time": "16h",
      "deliverables": ["API实现", "单元测试"],
      "dependencies": ["api_design"]
    },
    "frontend_dev": {
      "id": "frontend_dev",
      "type": "task",
      "title": "前端开发",
      "owner": "frontend_engineer",
      "estimated_time": "12h",
      "deliverables": ["前端页面", "前端测试"],
      "dependencies": ["ui_design"]
    },
    "integration": {
      "id": "integration",
      "type": "task",
      "title": "集成测试",
      "owner": ["backend_engineer", "frontend_engineer"],
      "estimated_time": "4h",
      "deliverables": ["集成测试报告"],
      "dependencies": ["backend_dev", "frontend_dev"]
    },
    "end": {
      "id": "end",
      "type": "end",
      "title": "项目完成"
    }
  },
  "edges": [
    {"from": "start", "to": "api_design"},
    {"from": "start", "to": "ui_design"},
    {"from": "api_design", "to": "backend_dev"},
    {"from": "ui_design", "to": "frontend_dev"},
    {"from": "backend_dev", "to": "integration"},
    {"from": "frontend_dev", "to": "integration"},
    {"from": "integration", "to": "end"}
  ],
  "collaboration_rules": {
    "auto_notify": {
      "task_complete": {
        "enabled": true,
        "template": "任务完成通知：{task_title} 已完成，交付物：{deliverables}"
      }
    }
  }
}
